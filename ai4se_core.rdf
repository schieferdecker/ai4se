<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/ai4se/owl#"
     xml:base="http://purl.org/ai4se/owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:gufo="http://purl.org/nemo/gufo#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:ai4se="http://purl.org/ai4se/owl#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://purl.org/ai4se/owl#">
        <owl:versionIRI rdf:resource="http://purl.org/ai4se/owl#/1.0.3"/>
        <owl:imports rdf:resource="http://purl.org/nemo/gufo#/1.0.0"/>
        <dc:creator>Schieferdecker, Ina K.</dc:creator>
        <dc:title xml:lang="en">ai4se: The AI for SE taxonomy</dc:title>
        <terms:bibliographicCitation xml:lang="en">Schieferdecker, I.K.: &quot;ai4se: A taxonomy of research papers on the support of software engineering (SE) through the use of artificial intelligence (AI) methods and techniques&quot;, 2025, http://purl.org/ai4se/doc</terms:bibliographicCitation>
        <terms:license>https://creativecommons.org/licenses/by/4.0/legalcode.en</terms:license>
        <vann:preferredNamespacePrefix xml:lang="en">ai4se</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/ai4se/owl/</vann:preferredNamespaceUri>
        <rdfs:comment xml:lang="en">ai4se is a taxonomy for research papers on the use of artificial intelligence (AI) methods and techniques to support software engineering (SE).  It relates papers to their topics and contributions. These contributions are categorised by the purpose of the solution aimed for, the SE activity or process targeted, the type of AI used and the level of automation supported.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</owl:versionInfo>
    </owl:Ontology>
    


