    <!-- http://purl.org/ai4se/owl#hasContribution -->

    <owl:ObjectProperty rdf:about="http://purl.org/ai4se/owl#hasContribution">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/ai4se/owl#isContributionOf"/>
        <rdfs:domain rdf:resource="http://purl.org/ai4se/owl#Research_paper"/>
        <rdfs:range rdf:resource="http://purl.org/ai4se/owl#Research_contribution"/>
        <rdfs:comment xml:lang="en">Property to link a research paper to its research contribution(s).</rdfs:comment>
        <rdfs:label>hasContribution</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ai4se/owl#isAboutResearchTopic -->

    <owl:ObjectProperty rdf:about="http://purl.org/ai4se/owl#isAboutResearchTopic">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ai4se/owl#Research_paper"/>
        <rdfs:range rdf:resource="http://purl.org/ai4se/owl#CSO:Topic"/>
        <rdfs:comment xml:lang="en">Property to link a research paper to its research topic(s).</rdfs:comment>
        <rdfs:label>isAboutResearchTopic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ai4se/owl#isContributionOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ai4se/owl#isContributionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ai4se/owl#Research_contribution"/>
        <rdfs:range rdf:resource="http://purl.org/ai4se/owl#Research_paper"/>
        <rdfs:label>is contribution of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ai4se/papers#isUsingModel -->

    <owl:ObjectProperty rdf:about="http://purl.org/ai4se/papers#isUsingModel">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ai4se/owl#SE_target"/>
        <rdfs:range rdf:resource="http://purl.org/ai4se/owl#Model"/>
        <rdfs:comment xml:lang="en">Property to link SE activities or processes to their models, in particular in Model-Driven Software Engineering</rdfs:comment>
        <rdfs:label>isUsingModel</rdfs:label>
    </owl:ObjectProperty>
    


