@prefix : <http://purl.org/ai4se/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/ai4se/ontology#> .

<http://purl.org/ai4se/ontology#> rdf:type owl:Ontology ;
                                   owl:versionIRI <http://purl.org/ai4se/ontology#/1.0.0> ;
                                   owl:imports <http://purl.org/nemo/gufo#/1.0.0> ;
                                   <http://purl.org/dc/elements/1.1/creator> "Schieferdecker, Ina K." ;
                                   <http://purl.org/dc/elements/1.1/title> "ai4se: A taxonomy of research papers on the support of software engineering (SE) through the use of artificial intelligence (AI) methods and techniques."@en ;
                                   <http://purl.org/dc/terms/bibliographicCitation> "Schieferdecker, I.K.: \"ai4se: A taxonomy of research papers on the support of software engineering (SE) through the use of artificial intelligence (AI) methods and techniques\", 2025, http://purl.org/ai4se/doc"@en ;
                                   <http://purl.org/dc/terms/license> "https://creativecommons.org/licenses/by/4.0/legalcode.en" ;
                                   <http://purl.org/vocab/vann/preferredNamespacePrefix> "ai4se"@en ;
                                   <http://purl.org/vocab/vann/preferredNamespaceUri> "http://purl.org/ai4se/ontology/"^^xsd:anyURI ;
                                   rdfs:comment "tbd"@en ,
                                                "An ontology of research papers generated from a BibTeX file." ;
                                   rdfs:label "AI for SE Ontology" ;
                                   owl:versionInfo 2 .

#################################################################
#    Object Properties
#################################################################

###  http://purl.org/ai4se/ontology#belongsTo
:belongsTo rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain <http://purl.org/nemo/gufo#AbstractIndividualType> ;
           rdfs:range <http://purl.org/nemo/gufo#AbstractIndividualType> .


###  http://purl.org/ai4se/ontology#belongsToAIType
:belongsToAIType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :belongsTo ;
                 rdfs:domain :AIType ;
                 rdfs:range :AIType .


###  http://purl.org/ai4se/ontology#belongsToSEActivity
:belongsToSEActivity rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :belongsTo ;
                     rdfs:domain :Target ;
                     rdfs:range :Target .


###  http://purl.org/ai4se/ontology#hasLevel
:hasLevel rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://purl.org/nemo/gufo#hasAssociatedQualityValueType> ;
          owl:inverseOf :levelEnabledBy ;
          rdfs:domain :ResearchPaper ;
          rdfs:range :AutomationLevel .


###  http://purl.org/ai4se/ontology#hasPurpose
:hasPurpose rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://purl.org/nemo/gufo#hasAssociatedQualityValueType> ;
            owl:inverseOf :purposeAdressedBy ;
            rdfs:domain :ResearchPaper ;
            rdfs:range :SolutionPurpose .


###  http://purl.org/ai4se/ontology#hasTarget
:hasTarget rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://purl.org/nemo/gufo#hasAssociatedQualityValueType> ;
           owl:inverseOf :targetFocussedBy ;
           rdfs:domain :ResearchPaper ;
           rdfs:range :Target .


###  http://purl.org/ai4se/ontology#hasType
:hasType rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://purl.org/nemo/gufo#hasAssociatedQualityValueType> ;
         owl:inverseOf :typeUsedBy ;
         rdfs:domain :ResearchPaper ;
         rdfs:range :AIType .


###  http://purl.org/ai4se/ontology#implementedIn
:implementedIn rdf:type owl:ObjectProperty ;
               rdfs:domain <http://purl.org/nemo/gufo#AbstractIndividualType> ;
               rdfs:range <http://purl.org/nemo/gufo#EndurantType> .


###  http://purl.org/ai4se/ontology#isAboutResearchTopic
:isAboutResearchTopic rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://purl.org/nemo/gufo#hasAssociatedQualityValueType> ;
                      owl:inverseOf :topicAddressedBy ;
                      rdfs:domain :ResearchPaper ;
                      rdfs:range :CSO:Topic ;
                      rdfs:comment "Represents the research topics that a research paper contributes to."@en ;
                      rdfs:label "isAboutResearchTopic" .


###  http://purl.org/ai4se/ontology#isProperPartOfProcess
:isProperPartOfProcess rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://purl.org/nemo/gufo#isEventProperPartOf> ;
                       rdfs:comment "Represents activities being part of processes."@en ;
                       rdfs:label "isPartOfProcess" .


###  http://purl.org/ai4se/ontology#isSEActivityWithModels
:isSEActivityWithModels rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://purl.org/nemo/gufo#mediates> ;
                        rdfs:range :Model .


###  http://purl.org/ai4se/ontology#levelEnabledBy
:levelEnabledBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :implementedIn ;
                rdfs:domain :AutomationLevel ;
                rdfs:range :ResearchPaper .


###  http://purl.org/ai4se/ontology#purposeAdressedBy
:purposeAdressedBy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :implementedIn ;
                   rdfs:domain :SolutionPurpose ;
                   rdfs:range :ResearchPaper .


###  http://purl.org/ai4se/ontology#targetFocussedBy
:targetFocussedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :implementedIn ;
                  rdfs:domain :Target ;
                  rdfs:range :ResearchPaper .


###  http://purl.org/ai4se/ontology#topicAddressedBy
:topicAddressedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :implementedIn ;
                  rdfs:domain :CSO:Topic ;
                  rdfs:range :ResearchPaper .


###  http://purl.org/ai4se/ontology#typeUsedBy
:typeUsedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :implementedIn ;
            rdfs:domain :AIType ;
            rdfs:range :ResearchPaper .


#################################################################
#    Classes
#################################################################

###  http://purl.org/ai4se/ontology#AIType
:AIType rdf:type owl:Class ;
        rdfs:subClassOf <http://purl.org/nemo/gufo#AbstractIndividualType> ;
        rdfs:comment """Represents the type of the artificial intelligence (AI) system being used.

According to ISO/IEC 22989:2022 Information technology - Artificial intelligence - Artificial intelligence concepts and terminology, artificial intelligence (AI) is

- <discipline> research and development of mechanisms and applications of AI systems.

and

artificial intelligence system (AI system) is

- engineered system that generates outputs such as content, forecasts, recommendations or decisions for a given set of human-defined objectives."""@en ;
        rdfs:label "Type of AI system" .


###  http://purl.org/ai4se/ontology#AutomationLevel
:AutomationLevel rdf:type owl:Class ;
                 rdfs:subClassOf <http://purl.org/nemo/gufo#AbstractIndividualType> ;
                 rdfs:label """Represents the level of automation provided / enabled by the AI system being used.

According to ISO/IEC 22989:2022 Information technology - Artificial intelligence - Artificial intelligence concepts and terminology, artificial intelligence (AI) is

- <discipline> research and development of mechanisms and applications of AI systems.

and

artificial intelligence system (AI system) is

- engineered system that generates outputs such as content, forecasts, recommendations or decisions for a given set of human-defined objectives."""@en ,
                            "Level of AI automation" .


###  http://purl.org/ai4se/ontology#Model
:Model rdf:type owl:Class ;
       rdfs:subClassOf <http://purl.org/nemo/gufo#Mixin> ;
       rdfs:comment """Represents artifacts defining (semi-)formally (1) properties of one or more artefacts used by software engineering activities or processes or (2) properties of software engineering activities or processes.

According to ISO/IEC 22989:2022 Information technology - Artificial intelligence - Artificial intelligence concepts and terminology, model is

- physical, mathematical or otherwise logical representation of a system, entity, phenomenon, process or data.

Note: For the AI4SE ontology, only mathematical or logical representations are relevant."""@en ;
       rdfs:label "Model" .


###  http://purl.org/ai4se/ontology#ResearchPaper
:ResearchPaper rdf:type owl:Class ;
               rdfs:subClassOf <http://purl.org/nemo/gufo#Mixin> ;
               rdfs:comment "Represents an artifact documenting research findings like new insights, methods, techniques, etc."@en ;
               rdfs:label "ResearchPaper" .


###  http://purl.org/ai4se/ontology#SolutionPurpose
:SolutionPurpose rdf:type owl:Class ;
                 rdfs:subClassOf <http://purl.org/nemo/gufo#AbstractIndividualType> ;
                 rdfs:comment "Represents the purpose of using the AI system in a software development / operation processes or activities."@en ;
                 rdfs:label "Solution purpose" .


###  http://purl.org/ai4se/ontology#Target
:Target rdf:type owl:Class ;
        rdfs:subClassOf <http://purl.org/nemo/gufo#AbstractIndividualType> ;
        rdfs:comment "Represents the primary concept, issue, component, software, or process in a software engineering process or activitiy being addressed."@en ;
        rdfs:label "Solution target" .


###  http://purl.org/ai4se/ontology#CSO:Topic
:CSO:Topic rdf:type owl:Class ;
           rdfs:subClassOf <http://purl.org/nemo/gufo#AbstractIndividualType> ;
           rdfs:isDefinedBy "https://cso.kmi.open.ac.uk/" ;
           rdfs:label "Represents the research topics in Computer Science as defined by the Computer Science Ontology CSO."@en ,
                      "CSO:Topic" .


#################################################################
#    Individuals
#################################################################

###  http://purl.org/ai4se/ontology#AI-assisted_options
:AI-assisted_options rdf:type owl:NamedIndividual ,
                              :AutomationLevel ;
                     rdfs:comment "Represents the case in which choices, recommendations, or decision-support outputs for the target SE processes or activities are generated or enhanced by an AI system, while retaining human control over the final choice, recommendation or decision, which may differ from the AI outputs."@en ;
                     rdfs:label "AI-assisted options" .


###  http://purl.org/ai4se/ontology#AI-assisted_selection
:AI-assisted_selection rdf:type owl:NamedIndividual ,
                                :AutomationLevel ;
                       rdfs:comment "Represents the case in which choices, recommendations, or decision-support outputs for the target SE processes or activities are generated or enhanced by an AI system, where the final choices, recommendations or decisions require human confirmation and do not differ from the AI outputs."@en ;
                       rdfs:label "AI-assisted selection" .


###  http://purl.org/ai4se/ontology#AI-driven_full_automation
:AI-driven_full_automation rdf:type owl:NamedIndividual ,
                                    :AutomationLevel ;
                           rdfs:comment "Represents the case in which AI is used to automate all tasks or decision-making components within the target SE processes or activities, without requiring human intervention."@en ;
                           rdfs:label "AI-driven full automation" .


###  http://purl.org/ai4se/ontology#AI-driven_partial_automation
:AI-driven_partial_automation rdf:type owl:NamedIndividual ,
                                       :AutomationLevel ;
                              rdfs:comment "Represents the case in which AI is used to automate specific tasks or decision-making components within the target SE processes or activities, while other parts of the process remain under human control or require human intervention."@en ;
                              rdfs:label "AI-driven partial automation" .


###  http://purl.org/ai4se/ontology#Agentic_AI
:Agentic_AI rdf:type owl:NamedIndividual ,
                     :AIType ;
            rdfs:comment "Represents AI systems that rely on autonomous agents using AI techniques to plan, adapt, and execute multi-step objectives with minimal human control."@en ;
            rdfs:label "Agentic AI" .


###  http://purl.org/ai4se/ontology#Architecture_and_Design
:Architecture_and_Design rdf:type owl:NamedIndividual ,
                                  :Target ;
                         :belongsToSEActivity :Develop ;
                         rdfs:comment """Represents software architecture and design activities and processes. 

A software architecture defines its fundamental organization, its components, their relationships, and the principles guiding its design and evolution. 
 
According to ISO/IEC/IEEE 42010:2022: Software, systems and enterprise - Architecture description), a software architecture consists of the fundamental concepts or properties of an entity in its environment and governing principles for the realization and evolution of this entity and its related life cycle processes.

Software design is the process of defining the architecture, components, interfaces, and other characteristics of a software or a component.

According to ISO/IEC/IEEE 24765: Systems and software engineering - Vocabulary, software design is  

1. the use of scientific principles, technical information, and imagination in the definition of a software system to perform pre-specified functions with maximum economy and efficiency."""@en ;
                         rdfs:label "Software architecture and design" .


###  http://purl.org/ai4se/ontology#Classical_machine_learning
:Classical_machine_learning rdf:type owl:NamedIndividual ,
                                     :AIType ;
                            :belongsToAIType :Subsymbolic ;
                            rdfs:comment """Represents subsymbolic AI systems that rely on statistical and algorithmic learning techniques - such as decision trees, support vector machines, linear regression, and clustering - to learn patterns from structured data and make predictions or classifications. These models typically operate on explicitly defined features and do not involve deep neural networks.

This class of AI is used in contrast to deep learning AI."""@en ;
                            rdfs:label "Classical machine learning" .


###  http://purl.org/ai4se/ontology#Coding
:Coding rdf:type owl:NamedIndividual ,
                 :Target ;
        :belongsToSEActivity :Develop ;
        rdfs:comment """Represents coding activities and processes. 

Coding (also known as programming) is the process of transforming the design of a software or a component into an executable computer program.

According to ISO/IEC/IEEE 24765: Systems and software engineering - Vocabulary, coding is

1. in software engineering, the process of expressing a computer program in a programming language

2. transforming of logic and data from design specifications (design descriptions) into a programming language."""@en ;
        rdfs:label "Software coding" .


###  http://purl.org/ai4se/ontology#Deep_learning
:Deep_learning rdf:type owl:NamedIndividual ,
                        :AIType ;
               :belongsToAIType :Subsymbolic ;
               rdfs:comment """Represents subsymbolic AI systems that rely on deep artificial neural networks to automatically learn features and perform tasks such as classification or prediction, especially from large and complex datasets. 

According to ISO/IEC 22989:2022 Information technology - Artificial intelligence - Artificial intelligence concepts and terminology, deep learning (deep neural network learning) is

- AI approach to creating rich hierarchical representations through the training of neural networks with many hidden layers."""@en ;
               rdfs:label "Deep learning AI" .


###  http://purl.org/ai4se/ontology#Deployment
:Deployment rdf:type owl:NamedIndividual ,
                     :Target ;
            :belongsToSEActivity :Operate ;
            rdfs:comment """Represents software deployment activities and processes.

Software deployment encompasses the processes and activities for placing a software or component into an operational environment.

According to ISO/IEC/IEEE 24765: Systems and software engineering - Vocabulary, deployment is 

1. a phase of a project in which a system is put into operation and cutover issues are resolved."""@en ;
            rdfs:label "Software deployment" .


###  http://purl.org/ai4se/ontology#Develop
:Develop rdf:type owl:NamedIndividual ,
                  :Target ;
         :belongsToSEActivity :Software_Engineering ;
         rdfs:label """Represents software development activities and processes.

Software development is the set of activities that result in software products, including requirements analysis, design, coding, testing, and maintenance. It encompasses all the activities and processes involved in creating, implementing, and modifying software (artefacts), as well as ensuring that it satisfies stakeholder requirements throughout the software development life cycle."""@en ,
                    "Develop" .


###  http://purl.org/ai4se/ontology#General_AI
:General_AI rdf:type owl:NamedIndividual ,
                     :AIType ;
            rdfs:comment """Represents AI systems capable of performing a broad set of distinct tasks with satisfactory performance.

According to ISO/IEC 22989:2022 Information technology - Artificial intelligence - Artificial intelligence concepts and terminology, general AI (AGI, artificial general intelligence)

- type of AI system that addresses a broad range of tasks with a satisfactory level of performance 

Note 1: Compared to narrow AI.
Note 2: AGI is often used in a stronger sense, meaning systems that not only can perform a wide variety of tasks, but all tasks that a human can perform."""@en ;
            rdfs:label "General AI" .


###  http://purl.org/ai4se/ontology#Generate
:Generate rdf:type owl:NamedIndividual ,
                   :SolutionPurpose .


###  http://purl.org/ai4se/ontology#Generative_AI
:Generative_AI rdf:type owl:NamedIndividual ,
                        :AIType ;
               rdfs:comment "Represents AI systems that use learned or derived knowledge to produce original outputs, such as synthetic text, media, or other content types. It often uses subsymbolic AI like Convolutional Neural Networks (CNNs), Recurrent Neural Networks (RNNs), or Transformer-based models, but also symbolic AI."@en ;
               rdfs:label "Generative AI" .


###  http://purl.org/ai4se/ontology#Improve
:Improve rdf:type owl:NamedIndividual ,
                  :SolutionPurpose ;
         rdfs:comment "The purpose of using AI is to improve (parts of) artefacts used in software engineering."@en ;
         rdfs:label "Improve" .


###  http://purl.org/ai4se/ontology#Improvement
:Improvement rdf:type owl:NamedIndividual ,
                      :Target ;
             :belongsToSEActivity :Process .


###  http://purl.org/ai4se/ontology#Maintenance
:Maintenance rdf:type owl:NamedIndividual ,
                      :Target ;
             :belongsToSEActivity :Develop ;
             rdfs:comment """Represents software maintenance activities and processes. 

Software maintenance is the process of modifying a software or component after delivery to correct faults, improve performance or other attributes, or adapt to a changed environment.

According to ISO/IEC/IEEE 24765: Systems and software engineering - Vocabulary, software maintenance is 

1. totality of activities required to provide cost-effective support to a software system 

2. entitlement of additional rights (such as additional
functionality, upgrade or support) for a previously granted software entitlement"""@en ;
             rdfs:label "Software maintenance" .


###  http://purl.org/ai4se/ontology#Management
:Management rdf:type owl:NamedIndividual ,
                     :Target ;
            :belongsToSEActivity :Process .


###  http://purl.org/ai4se/ontology#Monitoring
:Monitoring rdf:type owl:NamedIndividual ,
                     :Target ;
            :belongsToSEActivity :Operate ;
            rdfs:comment """Represents software monitoring activities and processes.

Software monitoring is systematically observing, checking, or keeping track of the operations or performance of a software or component."""@en ;
            rdfs:label "Software monitoring" .


###  http://purl.org/ai4se/ontology#No_AI_support
:No_AI_support rdf:type owl:NamedIndividual ,
                        :AutomationLevel ;
               rdfs:comment "Represents the case in which the target SE processes or activities are performed without the assistance of AI."@en ;
               rdfs:label "No AI-supported automation" .


###  http://purl.org/ai4se/ontology#Operate
:Operate rdf:type owl:NamedIndividual ,
                  :Target ;
         :belongsToSEActivity :Software_Engineering ;
         rdfs:comment "Represents software operation activities and processs."@en ;
         rdfs:label "Operate" .


###  http://purl.org/ai4se/ontology#Operation
:Operation rdf:type owl:NamedIndividual ,
                    :Target ;
           :belongsToSEActivity :Operate ;
           rdfs:comment """Represents software operation activities and processes.

Software operation refers to

1. The use and execution of software in its production environment to perform its intended functions.

2. Running a software to deliver services or perform tasks for users or other systems."""@en ;
           rdfs:label "Software operation" .


###  http://purl.org/ai4se/ontology#Process
:Process rdf:type owl:NamedIndividual ,
                  :Target ;
         :belongsToSEActivity :Software_Engineering ;
         rdfs:label "Represents processes for software engineering along the software lifecycle of software development and operation."@en ,
                    "Software Engineering Processes" .


###  http://purl.org/ai4se/ontology#Requirements
:Requirements rdf:type owl:NamedIndividual ,
                       :Target ;
              :belongsToSEActivity :Develop ;
              rdfs:comment """Represents requirements engineering activities and processes. 

A software requirement is a condition or capability that must be met by a software or component.

According to ISO/IEC/IEEE 24765: Systems and software engineering - Vocabulary, software requirement is

1. software capability needed by a user to solve a problem or to achieve an objective

2. software capability that must be met or possessed by a system or system component to satisfy a contract, standard, specification, or other formally imposed document

According to ISO/IEC/IEEE 24765: Systems and software engineering - Vocabulary, software requirements engineering is

1. science and discipline concerned with analyzing and documenting software requirements 

2. software requirements elicitation, analysis, specification, verification, and management"""@en ;
              rdfs:label "Requirements engineering" .


###  http://purl.org/ai4se/ontology#Software_Engineering
:Software_Engineering rdf:type owl:NamedIndividual ,
                               :Target .


###  http://purl.org/ai4se/ontology#Statistical
:Statistical rdf:type owl:NamedIndividual ,
                      :AIType ;
             :belongsToAIType :Subsymbolic ;
             rdfs:comment "Represents subsymbolic AI systems that rely on data and statistical inference to identify patterns and make predictions."@en ;
             rdfs:label "Statistical AI" .


###  http://purl.org/ai4se/ontology#Subsymbolic
:Subsymbolic rdf:type owl:NamedIndividual ,
                      :AIType ;
             rdfs:comment """Represents AI systems that use numerical models to represent knowledge and computations, such as numerical optimisation and statistical learning, in order to model and solve problems.

According to ISO/IEC 22989:2022 Information technology - Artificial intelligence - Artificial intelligence concepts and terminology, subsymbolic AI is

- AI based on techniques and models that use an implicit encoding of information, that can be derived from experience or raw data."""@en ;
             rdfs:label "Subsymbolic AI" .


###  http://purl.org/ai4se/ontology#Symbolic
:Symbolic rdf:type owl:NamedIndividual ,
                   :AIType ;
          rdfs:comment """Represents AI systems that use approaches relying on the explicit definition of symbols and rules representing knowledge and performing logical reasoning.

According to ISO/IEC 22989:2022 Information technology - Artificial intelligence - Artificial intelligence concepts and terminology, symbolic AI is

- AI based on techniques and models that manipulate symbols and structures according to explicitly defined rules to obtain inferences."""@en ;
          rdfs:label "Symbolic AI" .


###  http://purl.org/ai4se/ontology#Testing
:Testing rdf:type owl:NamedIndividual ,
                  :Target ;
         :belongsToSEActivity :Develop ;
         rdfs:comment """Represents software testing activities and processes.

Software testing encompasses the processes and activities used to evaluate the quality of a software or a component and of related artefacts.

According to ISO/IEC/IEEE 24765: Systems and software engineering - Vocabulary, software testing is

1. activity in which a system or component is executed under specified conditions, the results are observed or recorded, and an evaluation is made of some aspect of the system or component

2. dynamic verification that a program provides expected behaviors on a finite set of test cases, suitably selected from the usually infinite executions domain."""@en ;
         rdfs:label "Software testing" .


###  http://purl.org/ai4se/ontology#Understand
:Understand rdf:type owl:NamedIndividual ,
                     :SolutionPurpose ;
            rdfs:comment "The purpose of using AI is to gain insight."@en ;
            rdfs:label "Understand" .


