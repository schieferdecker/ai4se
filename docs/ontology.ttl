###  http://purl.org/ai4se/ontology#babur_models_2018
:babur_models_2018 rdf:type owl:NamedIndividual ,
                            :ResearchPaper ;
                   :hasLevel :No_AI_support ;
                   :hasPurpose :Understand ;
                   :hasTarget :Develop ;
                   :hasType :Classical_machine_learning ;
                   :isAboutResearchTopic :CSO:Model_driven_engineering ;
                   rdfs:comment """@inproceedings{babur_models_2018,
